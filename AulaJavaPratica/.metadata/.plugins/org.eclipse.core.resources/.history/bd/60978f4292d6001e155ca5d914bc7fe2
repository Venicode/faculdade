public class Aluna extends Pessoa{
	private int registroMatricula;
	public Aluna(String nome, int idade) {
		super(nome, idade);
		// TODO Auto-generated constructor stub
	}
	public void setRegistroMatricula(int registroMatricula) {
		this.registroMatricula = registroMatricula;
	}
	public int getRegistroMatricula() {
		return registroMatricula;
	}
	@override
	public void falar() {
		System.out.println("Ol√° sou uma aluna");
	}
}
